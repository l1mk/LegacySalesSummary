<% if @week.errors.any? %>
<div id="error_explanation">
  <h2>
    <%= pluralize(@week.errors.count, "error") %>
    prohibited this week from being saved:
  </h2>

  <ul>
  <% @week.errors.full_messages.each do |msg| %>
    <li><%= msg %></li>
  <% end %>
  </ul>
</div>
<% end %>

Month: <%= select_tag 'month_id', options_for_select(@months.collect{|u|[u.name, u.id]}) %><br>
Week Number: <%= f.number_field :week_number %><br>
<%= f.hidden_field :user_id, :value => current_user.id %>
<%= f.submit "Submit" %> 